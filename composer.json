{
  "name": "therealjohndough/compliant-theme",
  "description": "A modern, standards-compliant PHP theme with PHPUnit testing and code linting.",
  "type": "project",
  "require": {},
  "require-dev": {
    "phpunit/phpunit": "^10.5",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "App\\": "./"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "lint": "phpcs --standard=PSR12 ."
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
